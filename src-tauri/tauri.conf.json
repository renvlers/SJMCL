{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "SJMCL",
  "version": "0.0.0",
  "identifier": "com.sjmc.launcher",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "",
        "width": 800,
        "height": 550,
        "minWidth": 800,
        "minHeight": 550,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ]
  }
}
